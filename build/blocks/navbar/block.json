{
  "apiVersion": 3,
  "name": "codeweber-blocks/navbar",
  "version": "0.1.0",
  "title": "Navbar",
  "category": "codeweber-gutenberg-blocks",
  "icon": "menu-alt",
  "description": "Navbar block with multiple predefined header templates (Classic, Fancy, Extended, etc.)",
  "keywords": [
    "navbar",
    "header",
    "navigation",
    "menu"
  ],
  "supports": {
    "html": false,
    "customClassName": true,
    "anchor": true,
    "innerBlocks": true
  },
  "attributes": {
    "navbarType": {
      "type": "string",
      "default": "navbar-1"
    },
    "menuLocation": {
      "type": "string",
      "default": ""
    },
    "menuLocationRight": {
      "type": "string",
      "default": ""
    },
    "menuDepth": {
      "type": "number",
      "default": 4
    },
    "navbarColor": {
      "type": "string",
      "default": "light"
    },
    "logoColor": {
      "type": "string",
      "default": "auto"
    },
    "centerBarTheme": {
      "type": "string",
      "default": "auto"
    },
    "mobileOffcanvasTheme": {
      "type": "string",
      "default": "light"
    },
    "stickyNavbar": {
      "type": "boolean",
      "default": false
    },
    "transparentOnTop": {
      "type": "boolean",
      "default": false
    },
    "wrapperClass": {
      "type": "string",
      "default": ""
    },
    "navClass": {
      "type": "string",
      "default": ""
    },
    "blockClass": {
      "type": "string",
      "default": ""
    },
    "blockId": {
      "type": "string",
      "default": ""
    },
    "homeLink": {
      "type": "string",
      "default": ""
    },
    "logoWidth": {
      "type": "string",
      "default": ""
    },
    "logoHtmlEnabled": {
      "type": "boolean",
      "default": false
    },
    "logoHtml": {
      "type": "string",
      "default": "<p class=\"d-none d-lg-block ps-3 ms-3 h-100 border-start border-muted text-dark  mb-0\">Ваш надежный партнер<br>\nНа пути к результату</p>"
    },
    "headerBackground": {
      "type": "string",
      "default": ""
    },
    "headerBackgroundStyle": {
      "type": "string",
      "default": "solid"
    },
    "socialFromTheme": {
      "type": "boolean",
      "default": true
    },
    "socialStyleType": {
      "type": "string",
      "default": "type2"
    },
    "socialSize": {
      "type": "string",
      "default": "sm"
    },
    "socialButtonColor": {
      "type": "string",
      "default": "primary"
    },
    "socialButtonStyle": {
      "type": "string",
      "default": "solid"
    },
    "socialButtonForm": {
      "type": "string",
      "default": "circle"
    },
    "socialNavClass": {
      "type": "string",
      "default": ""
    },
    "socialThemeEnabledSlugs": {
      "type": "array",
      "default": []
    },
    "mobileMenuElements": {
      "type": "array",
      "default": []
    },
    "mobileMenuOffcanvasTheme": {
      "type": "string",
      "default": "light"
    },
    "mobileMenuSocialType": {
      "type": "string",
      "default": ""
    },
    "mobileMenuSocialSize": {
      "type": "string",
      "default": ""
    },
    "mobileMenuSocialStyle": {
      "type": "string",
      "default": ""
    }
  },
  "textdomain": "codeweber-gutenberg-blocks",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}