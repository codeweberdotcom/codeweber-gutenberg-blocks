{
  "apiVersion": 3,
  "name": "codeweber-blocks/blog-year-widget",
  "version": "0.1.0",
  "title": "Blog Year Widget",
  "category": "codeweber-gutenberg-widgets",
  "icon": "calendar",
  "description": "Widget list of monthly archive links (e.g. February 2019).",
  "keywords": [
    "blog",
    "archive",
    "year",
    "month",
    "widget",
    "sidebar"
  ],
  "supports": {
    "html": false,
    "customClassName": true,
    "anchor": true
  },
  "attributes": {
    "number": {
      "type": "number",
      "default": 12
    },
    "order": {
      "type": "string",
      "default": "DESC"
    }
  },
  "textdomain": "codeweber-gutenberg-blocks",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}