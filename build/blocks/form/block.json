{
  "apiVersion": 2,
  "name": "codeweber-blocks/form",
  "version": "0.1.0",
  "title": "Форма",
  "category": "codeweber-gutenberg-elements",
  "icon": "feedback",
  "description": "Создавайте контактные формы с разными типами полей",
  "supports": {
    "html": false,
    "customClassName": true,
    "anchor": true,
    "innerBlocks": true
  },
  "attributes": {
    "formId": {
      "type": "string",
      "default": ""
    },
    "recipientEmail": {
      "type": "string",
      "default": ""
    },
    "senderEmail": {
      "type": "string",
      "default": ""
    },
    "senderName": {
      "type": "string",
      "default": ""
    },
    "subject": {
      "type": "string",
      "default": ""
    },
    "successMessage": {
      "type": "string",
      "default": "Thank you! Your message has been sent."
    },
    "errorMessage": {
      "type": "string",
      "default": "An error occurred. Please try again."
    },
    "enableCaptcha": {
      "type": "boolean",
      "default": false
    },
    "captchaType": {
      "type": "string",
      "default": "honeypot"
    },
    "enableRateLimit": {
      "type": "boolean",
      "default": true
    },
    "rateLimitCount": {
      "type": "number",
      "default": 5
